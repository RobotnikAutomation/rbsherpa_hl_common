<?xml version="1.0"?>
<launch>

    <arg name="model" value="$(env ROBOT_MODEL)" />
    <arg name="sim" default="false"/>

      <rosparam file="$(find rbsherpa_hl_control)/config/rbsherpa_hl_control_$(arg model)_sim.yaml" command="load"/>

      	<!-- load the planar ros controllers by urdf -->
        <!-- Controller spawner for omni model -->
        <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
              output="screen" args="		  
              elevator_structure_controller
              torso_pantilt_pan_controller
              gun_tilt_controller
              gimbal_yaw_controller
              gimbal_roll_controller
              gimbal_pitch_controller
              joint_read_state_controller 
          ">
        </node>

</launch>
